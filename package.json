{
  "name": "eleventy-high-performance-blog",
  "version": "5.0.2",
  "license": "MIT",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "build-ci": "npm run js-build && eleventy && npm run test",
    "build": "npm run clean && npm run build-ci",
    "watch": "concurrently \"npm run serve\" \"npm run js-build-watch\" \"npm run test-watch\"",
    "serve": "npm run clean && eleventy --serve",
    "js-build": "rollup -c rollup.config.js",
    "js-build-watch": "rollup -c rollup.config.js -w",
    "debug": "DEBUG=* eleventy && npm run test",
    "test": "mocha test/test*.js",
    "test-watch": "mocha test/test*.js --watch --watch-files=test/*",
    "clean": "rm -Rf ./_site/posts/*"
  },
  "pre-push": [
    "build"
  ],
  "pre-commit": [],
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "browser-sync": "^3.0.2",
    "eleventy-plugin-local-images": "^0.4.0",
    "file-type": "^16.5.4",
    "fs-extra": "^11.2.0",
    "jsdom": "^16.5.0",
    "luxon": "^3.4.4",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.0.1",
    "node-fetch": "^2.6.7",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "shorthash": "0.0.2"
  },
  "dependencies": {
    "@ampproject/toolbox-optimizer": "^2.8.3",
    "any-shell-escape": "^0.1.1",
    "clean-css": "^5.3.3",
    "concurrently": "^8.2.2",
    "csp-hash-generator": "0.0.3",
    "csso": "^5.0.5",
    "datauri": "^4.1.0",
    "expect.js": "^0.3.1",
    "ffmpeg-static": "^5.2.0",
    "hasha": "^5.2.0",
    "html-minifier": "^4.0.0",
    "image-size": "^1.1.1",
    "jimp": "^0.22.12",
    "liquidjs": "^10.13.0",
    "lru-cache": "^10.2.2",
    "mocha": "^10.4.0",
    "phin": "^4.0.0",
    "purge-from-html": "^1.0.3",
    "purgecss": "^6.0.0",
    "qs": "^6.12.1",
    "querystring": "^0.2.1",
    "rollup": "^2.79.1",
    "sharp": "^0.33.4",
    "terser": "^5.31.0",
    "touch": "^3.1.1",
    "twitter": "^1.7.1",
    "ua-parser-js": "^1.0.37"
  }
}
